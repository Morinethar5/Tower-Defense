using UnityEngine;
using UnityEngine.SceneManagement;

public class MainMenu : MonoBehaviour {

	public string levelToLoad = "_Level01";

	public void Play() {
		//Debug.Log ("Play");
		SceneManager.LoadScene(levelToLoad);
	}

	public void Quit() {
		Debug.Log ("Exiting game...");
		Application.Quit ();
	}
}